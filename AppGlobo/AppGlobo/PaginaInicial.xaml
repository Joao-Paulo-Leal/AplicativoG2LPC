										<RichTextBox VerticalAlignment="Top">
											<Paragraph TextAlignment="Justify">
												<Run  Text="{Binding title}"  
											Foreground="White"/>
											</Paragraph>
										</RichTextBox>
										<RichTextBox VerticalAlignment="Top">
											<Paragraph TextAlignment="Justify">
												<Run Text="{Binding description}" 
											Foreground="White"/>
											</Paragraph>
										</RichTextBox>
										<TextBlock 
									Text="{Binding pubDate}" 
